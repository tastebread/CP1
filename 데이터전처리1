import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt
df = pd.read_csv('tourism.csv',encoding='utf-8')

print(df)
print(df.shape)

df1 = df.transpose()

df1 = pd.read_csv('tourismTranspose.csv',encoding='utf-8')

print(df1.iloc[1:])
print(df1.columns)

plt.rcParams['font.family'] = 'AppleGothic'
#sns.barplot(data=df1, x='지역',y='지출액(소계)')
#plt.savefig('date,counts(barplot).png')
sns.swarmplot(data=df1, x='지역',y='지출액(소계)')
plt.savefig('date,counts(swarmplot.png')
